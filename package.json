{
  "name": "yc-actions-yc-iam-token",
  "version": "1.2.0",
  "description": "GitHub Action issuing Yandex Cloud IAM Token.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "git-tag": "git tag v`cat package.json | jq -r '.version' | awk -F. '{print $1}'` -f &&  git tag v`cat package.json | jq -r '.version'` -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yc-actions/yc-iam-token.git"
  },
  "keywords": [
    "actions",
    "yandex cloud",
    "login"
  ],
  "author": "Nikolay Matrosov",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@yandex-cloud/nodejs-sdk": "2.9.0"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@types/stream-buffers": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vercel/ncc": "^0.38.4",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "^30.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
